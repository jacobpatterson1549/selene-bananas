CREATE OR REPLACE FUNCTION user_update_points
	( INOUT username VARCHAR
	, IN points INT
	) RETURNS SETOF VARCHAR
AS
$$
	WITH updated AS
		(
		UPDATE users
		AS u
		SET points = user_update_points.points
		WHERE u.username = user_update_points.username
		RETURNING u.username
		)
	SELECT username
	FROM updated
$$
LANGUAGE SQL;
